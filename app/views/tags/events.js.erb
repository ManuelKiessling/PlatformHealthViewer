<% if @events then %>
  $("#events_tagid_<%= @tagid %>").hide()
  $("#events_tagid_<%= @tagid %>").html('')
  <% @events.each  do |event| %>
    $("#events_tagid_<%= @tagid %>").append('<%= escape_javascript(render :partial => "event", :locals => { :event => event } ) %>');
  <% end %>
  $("#events_tagid_<%= @tagid %>").slideDown(400);
<% end %>
